{
  "name": "hybrid-array-runner",
  "version": "1.0.0",
  "description": "ES6 Class which enables running functions returning promises in a combination serial and parallel mode for each element in an array, capturing the individual resolved data.",
  "main": "./lib/hybrid-array-runner.js",
  "directories": {
    "lib": "./lib",
    "doc": "./doc",
    "test": "./test",
    "example": "./example"
  },
  "scripts": {
    "test": "mocha -t 10000 --async-only --reporter spec --ui bdd test",
    "generateDocs": "node_modules/.bin/jsdoc lib/hybrid-array-runner.js lib/array-runner.js -d doc"
  },
  "keywords": [
    "asynchronous",
    "array",
    "hybrid",
    "runner",
    "es6"
  ],
  "author": "Bill Hodges",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/wildbillh/hybrid-array-runner"
  },
  "dependencies": {
    "parallel-array-runner": "^1.0.0",
    "serialized-array-runner": "^1.1.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "jsdoc": "^3.4.2"
  }
}
